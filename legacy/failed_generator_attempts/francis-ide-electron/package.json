{
  "private": true,
  "name": "francis-ide-electron",
  "version": "0.0.1",
  "main": "lib/backend/electron-main.js",
  "dependencies": {
    "@theia/core": "1.67.0",
    "@theia/editor": "1.67.0",
    "@theia/electron": "1.67.0",
    "@theia/filesystem": "1.67.0",
    "@theia/messages": "1.67.0",
    "@theia/monaco": "1.67.0",
    "@theia/workspace": "1.67.0",
    "@theia/variable-resolver": "1.67.0",
    "reflect-metadata": "^0.2.2",
    "francis-ide-extension": "workspace:0.0.1"
  },
  "devDependencies": {
    "@theia/cli": "1.67.0",
    "electron": "38.4.0",
    "umd-compat-loader": "^2.1.2",
    "css-loader": "^7.1.2",
    "style-loader": "^4.0.0",
    "source-map-loader": "^5.0.0"
  },
  "scripts": {
    "build": "npm run bundle",
    "bundle": "npm run rebuild && theia build --mode development",
    "rebuild": "theia rebuild:electron --cacheRoot ..",
    "start": "theia start",
    "watch": "npm run rebuild && theia build --watch --mode development"
  },
  "theia": {
    "target": "electron",
    "generator": {
      "exclude": [
        "@theia/navigator",
        "@theia/markers",
        "@theia/outline-view",
        "@theia/terminal"
      ]
    }
  }
}
