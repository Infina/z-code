{
  "private": true,
  "name": "francis-ide-browser",
  "version": "0.0.1",
  "dependencies": {
    "@theia/core": "1.67.0",
    "@theia/editor": "1.67.0",
    "@theia/filesystem": "1.67.0",
    "@theia/messages": "1.67.0",
    "@theia/monaco": "1.67.0",
    "@theia/variable-resolver": "1.67.0",
    "@theia/workspace": "1.67.0",
    "bindings": "^1.5.0",
    "francis-ide-extension": "workspace:0.0.1",
    "reflect-metadata": "^0.2.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@theia/cli": "1.67.0",
    "css-loader": "^7.1.2",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "umd-compat-loader": "^2.1.2"
  },
  "scripts": {
    "build": "npm run bundle",
    "bundle": "npm run rebuild && theia build --mode development",
    "rebuild": "theia rebuild:browser --cacheRoot ..",
    "start": "theia start",
    "start:dev": "node src-gen/backend/main.js",
    "watch": "npm run rebuild && theia build --watch --mode development"
  },
  "theia": {
    "target": "browser",
    "generator": {
      "exclude": [
        "@theia/navigator",
        "@theia/markers",
        "@theia/outline-view",
        "@theia/terminal"
      ]
    }
  }
}
